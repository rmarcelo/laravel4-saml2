{
    "name": "pitbulk/laravel4-saml2",
    "description": "A Laravel 4.2 package for Saml2 integration as a SP (service provider) based on OneLogin toolkit, which is much lightweight than simplesamlphp",
    "keywords": ["laravel","saml", "saml2", "onelogin"],
    "homepage": "https://github.com/pitbulk/laravel4-saml2/",
    "license": "MIT",
    "version" : "0.0.5",
    "authors": [
        {
            "name": "pitbulk",
            "email": "sixto.martin.garcia@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "onelogin/php-saml": "dev-remove_mcrypt"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Pitbulk\\Saml2\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
